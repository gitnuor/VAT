{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "bootstrap-select@1.14.0-beta3",
      "destination": "wwwroot/lib/bootstrap-select/"
    },
    {
      "library": "bootstrap-icons@1.11.1",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "moment.js@2.29.4",
      "destination": "wwwroot/lib/moment-js/"
    },
    {
      "library": "print-js@1.6.0",
      "destination": "wwwroot/lib/print-js/"
    },
    {
      "library": "typeahead.js@0.11.1",
      "destination": "wwwroot/lib/typeahead.js/"
    },
    {
      "library": "imask@7.1.3",
      "destination": "wwwroot/lib/imask/"
    },
    {
      "provider": "unpkg",
      "library": "toastr@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "toastr.scss",
        "toastr.js"
      ]
    }
  ]
}