@model IEnumerable<vms.entity.models.OrgBranch>
@(Html.Grid(Model).Build(columns =>
{
    columns.Add(model => model.Name).Titled("Branch Name");
    columns.Add(model => model.BusinessCategoryDescription).Titled("Description");
    columns.Add(model => model.EmailAddress).Titled("Email");

    columns.Add(model => model.Mobile).Titled("Mobile");
    columns.Add(model => model.Address).Titled("Address");
    columns.Add(model => model.Status).Titled("Status");
	columns.Add().Encoded(false).RenderedAs(data => Html.PartialAsync("_ActionMenu", data).Result)
		.Titled(Html.Raw("<i class='bi bi-activity'></i>")).AppendCss("action-col-freez");
})
	.AppendCss("large-index-grid")
	.Empty("No data found")
	.Filterable(GridFilterType.Double)
	.Sortable()
	.Pageable())