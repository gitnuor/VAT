    <div class="row mt-2">
        <div class="col-12">
            @{
                var sfKas = Model.MushakReturnSubFormKaList.Select(sfk => sfk.SubFormName).Distinct();

                foreach (var sfka in sfKas)
                {
                    <div id="subFormKa" style="font-weight:bold !important"><div class="parthead">@sfka</div></div>
                    <partial name="MushakReturn/_ka" model="Model.MushakReturnSubFormKaList.Where(sfk => sfk.SubFormName == sfka)" />

                }
            }
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-12">
            <partial name="MushakReturn/_kha" model="Model.MushakReturnSubFormKhaList" />
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-12">
            @{
                var sfKas3 = Model.MushakReturnSubFormGaList.Select(sfk => sfk.SubFormName).Distinct();

                foreach (var sfka3 in sfKas3)
