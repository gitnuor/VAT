@model vms.entity.viewModels.ReportsViewModel.VmMushakReturn


<div class="row ivat-bangla-font">
    <div class="col-2">
        @*@if (!string.IsNullOrEmpty(firstElement.InvoiceStatus))
            {<div class="mushak-name" style="float:left;">@firstElement.InvoiceStatus</div>}*@
        <img src="/images/logo_bd_govt.svg" height="80px" width="80px" />
