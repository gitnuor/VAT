@model IEnumerable<vms.entity.StoredProcedureModel.MushakReturn.MushakReturnSubFormUma>
<div class="parthead" id="note29SubForm">
    <b>Subform- ṅ (For Note- 29 VDS Deposit)</b>
</div>
<table class="table table-bordered wrap">

    @*<thead>*@

        <tr class="MreturnHeader" style="background-color: #FBD4B4!important">

            <th>SL</th>
            <th>
                Customer BIN
            </th>
            <th>
                Customer Name
            </th>
            <th>Customer Address</th>
            <th>
                Value
            </th>

            <th>
                Deducted VAT
            </th>
            <th>
                Tax Challan No.
            </th>
            <th>Tax Challan Date</th>
            <th>
                VDS Certificate No.
            </th>
            <th>
                VDS Certificate Date
            </th>
            <th>
                Tax Deposit Account Code
            </th>
            <th>
               VDS Deposit Book Transfer No.
            </th>
            <th>
                VDS Deposit Book Transfer Date
            </th>
            <th>
               Note
            </th>
        </tr>
    @*</thead>
    <tbody>*@
        @if (Model.Any())
        {

            foreach (var item in Model)
            {
                <tr>
                    <td>@item.SlNo</td>
                    <td>@item.CustomerBinNo</td>
                    <td>@item.CustomerName</td>
                    <td>@item.CustomerAddress</td>
                    <td class="text-right" style="text-align:right!important">@StringGenerator.DecimalToCommaSeparated(item.Price)</td>
                    <td class="text-right" style="text-align:right!important">@StringGenerator.DecimalToCommaSeparated(item.DeductedVat)</td>
                    <td>@item.VatChallanNo</td>
                    <td>@item.TaxInvoicePrintedTime</td>
                    <td>@item.VDSCertificateNo </td>
                    <td>@StringGenerator.DateTimeToString(item.VDSCertificateIssueTime) </td>
                    <td>@item.EconomicCode</td>
                    <td>@item.VDSPaymentBookTransferNo</td>
                    <td>@StringGenerator.DateTimeToString(item.VDSPaymentDate, false)</td>
                    <td>@item.Note</td>
                </tr>

            }
        }
    @*</tbody>
    <tfoot>*@
        <tr class="MreturnHeader" style="background-color: #FBD4B4!important">

            <th></th>
            <th></th>
            <th></th>
            <th class="text-right" style="text-align:right!important">Total</th>
            <th class="text-right" style="text-align:right!important">@StringGenerator.DecimalToCommaSeparated(Model.Sum(c => c.Price))</th>
            <th class="text-right" style="text-align:right!important">@StringGenerator.DecimalToCommaSeparated(Model.Sum(c => c.DeductedVat))</th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    @*</tfoot>*@
</table>