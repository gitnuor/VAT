@model IEnumerable<vms.entity.StoredProcedureModel.MushakReturn.MushakReturnSubFormCha>

<div class="parthead" id="subFormCa"> <b>Subform- c (Note 30- For "Advance Tax (AT) Payment")</b></div>
<table class="table table-bordered">

   

        <tr class="MreturnHeader" style="background-color: #FBD4B4!important">

            <th>SL</th>
            <th>
                Bill of Entry No.
            </th>
            <th>
                Date
            </th>
            <th>Customs and VAT Commissionerate</th>
            <th style="text-align:right !important">
                Amount
            </th>

            <th>Remarks</th>
        </tr>
  
        @if (Model.Any())
        {

            foreach (var item in Model)
            {
                <tr>

                    <td class="text-right">@item.SlNo</td>
                    <td class="text-center">@item.BillOfEntryNo</td>
                    <td>@StringGenerator.DateTimeToStringWithoutTime(item.BillOfEntryDate)</td>
                    <td>@item.CustomsAndVATCommissionarateName</td>
                    <td class="text-right" style="text-align:right !important">@StringGenerator.DecimalToCommaSeparated(item.AdvanceTaxPaidAmount)</td>
                    <td></td>
                </tr>

            }
        }
    
        <tr class="MreturnHeader" style="background-color: #FBD4B4!important">

            <th></th>
            <th></th>
            <th></th>
            <th class="text-right">Total</th>
            <th class="text-right">@StringGenerator.DecimalToCommaSeparated(Model.Sum(c => c.AdvanceTaxPaidAmount))</th>
            <th></th>
        </tr>
   
</table>