@model vms.entity.StoredProcedureModel.MushakReturn.MushakReturnPartTwo

<table class="table table-bordered">
    @*<thead>*@

        <tr>

            <th class="MreturnHeader" colspan="3" style="background-color: #FBD4B4!important">

                <div class="parthead text-center"> Part-2 : RETURN SUBMISSION DATA</div>
            </th>
        </tr>
    @*</thead>
    <tbody class="Returntd">*@

        <tr>
            <td class="tdp1">(1) Tax Period </td>
            <td class="tdp2"> :</td>
            <td>@Model.TermOfTaxEng</td>
        </tr>

        <tr>
            <td>
                (2) Type of Return
                [Please select your desired option]
            </td>
            <td class="tdp2"> :</td>
            <td>
            @*@Model.TypeOfSubmission*@
                <table class="inner-table">
                    <tr>
                        <td>(k) Main/Orginal Return  (Section 64)</td>
                        <td><input type="checkbox" name="" value="" checked /></td>
                    </tr>
                    <tr>
                        <td>(kh) Late Return (Section 65)</td>
                        <td><input type="checkbox" name="" value="" /></td>
                    </tr>
                    <tr>
                        <td>(g) Amended Return (Section 66) </td>
                        <td><input type="checkbox" name="" value="" /></td>
                    </tr>
                    <tr>
                        <td>(gh) Full or Additional or Alternative Return (Section 67) </td>
                        <td><input type="checkbox" name="" value="" /></td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td>(৩) Any activities in this Tax Period? </td>
            <td class="tdp2"> :</td>
            <td>
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="IsTransactionOccuredInLastTermYes" disabled="disabled" checked="@(Model.IsTransactionOccuredInLastTerm == true)">
                    <label class="form-check-label" for="IsTransactionOccuredInLastTermYes">Yes</label>
                    
                    <input type="checkbox" style="margin-left: 9px;" disabled="disabled" checked="@(Model.IsTransactionOccuredInLastTerm != true)">
                    <label class="form-check-label" for="IsWantToGetBackClosingAmountNo">No</label>
                </div>
                <div>[If Selected "No" please Fill Part- 1, 3 11] </div>
            </td>
        </tr>

        <tr>
            <td>(4) Date of Submission</td>
            <td class="tdp2"> :</td>
            <td>@StringGenerator.DateTimeToString(Model.SubmissionDate, false)</td>
        </tr>
    @*</tbody>*@
</table>