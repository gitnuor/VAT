@model vms.entity.models.OrgStaticDatum


@{
    ViewData["Title"] = "Create";
    
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="custom-page-title">Add Adjustment Type</h1>
    </div>
</div>


<div class="row">
    <div class="col-md-12">
        <form asp-action="AdjustmentTypeCreate" class="form-horizontal" id="frmDamageCreate">
            <div class="form-body">
                <div class="form-group required">
                    <label class="col-md-3 control-label intiLabel">Adjustment Type</label>
                    <div class="col-md-4">
                        <select id="DataKey" name="DataKey" class="form-control" asp-items="ViewBag.AdjustmentTypeId" required>
                            <option value="">Select</option>
                        </select>
                        <span asp-validation-for="DataKey" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 control-label intiLabel"> Reference Key : </label>
                    <div class="col-md-4">
                        <input asp-for="ReferenceKey" class="form-control" maxlength="10"/>
                        <span asp-validation-for="ReferenceKey" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="form-actions">
                <div class="row pl-3">
                    <div class="col-md-offset-3 col-md-9">
                        <button type="submit" data-requiredconfirmation="true" class="btn  green"><i class="bi bi-check-lg"></i> Submit</button>
                        <button type="button" value="Reset" onclick="location.reload();" class="btn  btn-warning btn-sm"><i class="bi bi-arrow-repeat"></i> Reset</button>
                        <a asp-action="AdjustmentTypeIndex" class="btn btn-secondary btn-sm"><i class="bi bi-slash-circle"></i> Cancel</a>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>



