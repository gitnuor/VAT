@model vms.entity.models.AuditLog

@{
    ViewData["Title"] = "Details";
}

@section contentheader
{
    <div class="p-3 page-header">
        Audit Details
    </div>
}

<div class="row">

    <div class="col-md-12 ">
        <table class="table table-bordered">
            <tbody>
                <tr>
                    <th style="width: 150px;">
                        Table Name
                    </th>
                    <td>
                        @Model.ObjectType.Name
                    </td>
                </tr>
                <tr>
                    <th>
                        Change Type
                    </th>
                    <td>
                        @Model.AuditOperation.Name
                    </td>
                </tr>
                <tr>
                    <th>
                        Description
                    </th>
                    <td>
                        @Model.Descriptions
                    </td>
                </tr>
                <tr>
                    <th>
                        Change Time
                    </th>
                    <td>
                        @Model.CreatedTime
                    </td>
                </tr>
                <tr>
                    <th>
                        User
                    </th>
                    <td>
                        @Model.User.FullName (@Model.User.UserName)
                    </td>
                </tr>
            </tbody>
        </table>
        <a asp-action="Index" class="btn btn-secondary"><i class="bi bi-chevron-double-left"></i> Back</a>

    </div>
</div>