                            <label asp-for="BankId" ></label>
                            <select asp-for="BankId" asp-items="Model.BankList.CustomSelectListToSelectList()" >
                                <option value="@null">Select Bank</option>
                            </select>
                            <span asp-validation-for="BankId" ></span>
