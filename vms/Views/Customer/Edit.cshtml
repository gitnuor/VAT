                    <button type="submit" data-requiredconfirmation="true" class="btn btn-primary btn-sm"><i class="bi bi-check-lg"></i> Update</button>
                    <button type="button" value="Reset" onclick="location.reload();" class="btn  btn-warning btn-sm"><i class="bi bi-arrow-repeat"></i> Reset</button>
                    <a asp-controller="Customer" asp-action="Index" class="btn btn-secondary btn-sm"><i class="bi bi-slash-circle"></i> Cancel</a>
