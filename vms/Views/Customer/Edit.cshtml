@model vms.entity.viewModels.vmCustomer

@{
    ViewData["Title"] = "Edit Customer";
    
    
}

@section contentheader
    {
    <div class="p-3 page-header">
        @ViewData["Title"]
    </div>
}

<div class="row">
    <div class="col-md-8 offset-2">

