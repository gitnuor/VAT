<div class="accordion mt-3" id="accordionForm">
    <div class="accordion-item">
        <div class="accordion-header" id="headingOtherInformation">
            <button class="accordion-button collapsed vms-accordion-button"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target="#otherInformation"
                    aria-expanded="false"
                    aria-controls="otherInformation">
                Others Information
            </button>
        </div>
        <div id="otherInformation"
