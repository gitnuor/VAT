
@model vms.entity.StoredProcedureModel.SpAddMushakReturnSubmissionInfo

@{
    ViewData["Title"] = "AddMushakSubmissionInfo";
    
}
<div class="row">
    <div class="col-lg-12 mb-3">
        <h1 class="custom-page-title">Add Mushak Submission Info</h1>
    </div>
</div>

<div class="row">
    <div class="col-md-12">

        <form asp-action="AddMushakReturnSubmissionInfo" class="form-horizontal">
            @Html.HiddenFor(c => c.MushakGenerationId)
            <div class="form-body">
                <div class="form-group">
                    <label class="col-md-3 control-label">Submission Date : </label>
                    <div class="col-md-4">
                        
                        <input type="text" id="SubmissionDate" name="SubmissionDate" class="form-control" value="@Model.SubmissionDate"/>
                    </div>
                </div>

            </div>

            <div class="form-actions">
                <div class="row">
                    <div class="col-md-offset-3 col-md-9">
                        <button type="submit" data-requiredconfirmation="true" class="btn  green">Submit</button>

                        <a href="~/MushakGenerations/Index" class="btn btn-info btn-sm">Cancel</a>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>

@section Scripts{


    <script src="~/js/jquery-ui.js"></script>
    <script>

        $(document).ready(function() {

            $("#SubmissionDate").datepicker();
            $("#SubmissionDate").datepicker('option', 'dateFormat', 'dd M, yy');
          
        });
    </script>
}
